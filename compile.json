{
   "$schema": "https://qooxdoo.org/schema/compile-1-0-0.json",
   "environment": {
    "qx.icontheme": "Tango"
   },
	"applications": [
		{
			"class": "qxl.formdemo.Application",
			"theme": "qx.theme.Modern",
			"name": "formdemo",
			"title": "Qooxdoo Form Demo"
		}
	],
	/** Targets */
	"targets": [
		{
			"type": "source",
			"outputPath": "compiled/source"
		},
		{
			"type": "build",
			"outputPath": "compiled/build"
		}
	],
	"defaultTarget": "source",
	"locales": [
		"en"
	],
	"serve": {
	   "listenPort": 8080
	}
}